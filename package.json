{
  "name": "lunox",
  "version": "0.4.1",
  "description": "Laravel-Flavoured NodeJs framework",
  "bin": {
    "lunox": "./bin/lunox.cjs"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist/*",
    "helpers.d.ts"
  ],
  "scripts": {
    "build": "npm run clean && NODE_ENV=production rollup -c && npm run fix",
    "clean": "rm -rf dist & rm -rf bin",
    "dev": "rollup -cw & nodemon -q -w dist --es-module-specifier-resolution=node dist/index.js",
    "fix": "rm dist/Types.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "prettier": "prettier --write --plugin-search-dir=. .",
    "format": "npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kodepintar/lunox-framework.git"
  },
  "keywords": [
    "laravel",
    "nodejs",
    "lunox",
    "polka",
    "svelte",
    "mvc"
  ],
  "author": {
    "name": "Akhmad Salafudin",
    "email": "axmad386@gmail.com",
    "url": "https://github.com/axmad386"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kodepintar/lunox-framework/issues"
  },
  "homepage": "https://github.com/kodepintar/lunox-framework#readme",
  "dependencies": {
    "@slynova/flydrive": "^1.0.3",
    "colorette": "^2.0.16",
    "commander": "^8.3.0",
    "dotenv": "^10.0.0",
    "formidable": "^2.0.1",
    "node-input-validator": "^5.0.0-beta.3",
    "pluralize": "^8.0.0",
    "polka": "^1.0.0-next.22",
    "sirv": "^1.0.18"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@slynova/flydrive-s3": "^1.0.3",
    "@types/cors": "^2.8.12",
    "@types/formidable": "^2.0.0",
    "@types/node": "^16.11.7",
    "@types/pluralize": "^0.0.29",
    "@types/polka": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "knex": "^0.95.14",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.15",
    "objection": "^3.0.0",
    "prettier": "^2.4.1",
    "rollup": "^2.60.1",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "vite": "^2.6.14"
  },
  "type": "module"
}
